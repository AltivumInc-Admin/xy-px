{
  "name": "xy-px",
  "version": "1.2.3",
  "description": "React cursor coordinates tracker for precise LLM communication. Perfect for AI-assisted development with ChatGPT, Claude, and Copilot. Features real-time display, cross-platform support, and instant copy-paste.",
  "main": "dist/auto-inject.js",
  "module": "dist/auto-inject.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "*.css",
    "**/auto-inject.*"
  ],
  "browser": "dist/auto-inject.esm.js",
  "exports": {
    ".": {
      "import": "./dist/auto-inject.esm.js",
      "require": "./dist/auto-inject.js",
      "types": "./dist/index.d.ts"
    },
    "./manual": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auto": {
      "import": "./dist/auto-inject.esm.js",
      "require": "./dist/auto-inject.js",
      "types": "./dist/auto-inject.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "bin",
    "scripts/postinstall.js"
  ],
  "bin": {
    "xy-px": "./bin/init.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "demo": "vite demo",
    "demo:build": "vite build demo",
    "prepublishOnly": "npm run build && npm run verify-package",
    "verify-package": "node scripts/verify-package.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "react",
    "cursor",
    "coordinates",
    "position",
    "tracking",
    "debug",
    "development",
    "tool",
    "viewport",
    "scroll",
    "cross-platform",
    "touch",
    "mobile",
    "macos",
    "windows",
    "ios",
    "android",
    "xy",
    "px",
    "llm",
    "ai",
    "chatgpt",
    "claude",
    "copilot",
    "ai-development",
    "ai-assisted",
    "pixel-perfect",
    "positioning",
    "ui-development",
    "developer-tools"
  ],
  "author": "Altivum Inc <admin@altivum.io>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-css-only": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AltivumInc-Admin/xy-px.git"
  },
  "bugs": {
    "url": "https://github.com/AltivumInc-Admin/xy-px/issues"
  },
  "homepage": "https://github.com/AltivumInc-Admin/xy-px#readme"
}
